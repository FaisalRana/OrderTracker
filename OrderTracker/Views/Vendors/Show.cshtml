@using OrderTracker.Models;

<h3>Here are all the orders with this Vendor:</h3>
<ol>

@foreach (Order order in @Model["Orders"])
{
  <li><a href='/vendors/@Model["Vendor"].Id/orders/@order.Id'>@order.Title</a></li>
}
</ol>


<h3><a href='/vendors/@Model["Vendor"].Id/orders/new'>Add an order to this vendor</a></h3>
<h3><a href='/vendors'>Back to all Vendors List</a></h3>
<h3><a href='/'>Back to Home Page</a></h3>



